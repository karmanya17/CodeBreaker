<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">

  <!-- Bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">

  <!-- StyleSheets -->
  <link rel="stylesheet" href="styles.css">
  <link rel="stylesheet" href="history.css">

  <!-- FontAwesome -->
  <script src="https://kit.fontawesome.com/54ff6d8ec9.js" crossorigin="anonymous"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

   <title>History</title>

</head>

<body>

  <!-- Left Nav -->
    <%- include('partials/leftnav') %>

<!-- Content -->


    <div class="content">
<section id="history">
  <div class="history-nav">
      <button onclick="goBack()" class="previous round" style="border:none;background:#E5E5E5"><i class="fas fa-chevron-left" ></i></button>
      <img class="heading-logo inline"src="images/50.png" alt="heading-logo">
      <h3 class="history-heading inline">QUESTIONS HISTORY</h3>
    </div>

    <!-- History Container -->

    <div class="history-container">
      <% for(let i=0;i<11;i++) { %>
        <!-- <%  console.log(language[i])%> -->
        <%  if ( statusinfo[i] === "WRONG_ANSWER" ||statusinfo[i]=== "COMPILATION_ERROR" ){ %>

        <div class="history-container-div wrong-color ">
          <div class="history-container-platform-logo-div inline">
            <img class="history-container-platform-logo inline"src="images/LeetCode logo.png" alt="platform-logo">
          </div>
          <div class="checkoverflow inline margin-L-2 width-30">
            <a href="https://codeforces.com/problemset/problem/<%= problemId[i] %>/<%= problemIndex[i] %>" class=" "><%= history[i] %></a>
          </div>

          <h6 class="inline margin-L-1 center ">LANGUAGE :</h6>
          <% if(language[i]==="GNU C++14" ||language[i]==="GNU C++11" ||language[i]==="GNU C++17"){ %>
          <h6 class="inline">C++</h6>
        <%  } %>
        <% if(language[i]==="GNU C11" ){ %>
        <h6 class="inline">C</h6>
      <%  } %>
        </div>
    <% } else if ( statusinfo[i]=== "TIME_LIMIT_EXCEEDED" ){ %>
      <div class="history-container-div TLE-color ">
        <div class="history-container-platform-logo-div inline">
          <img class="history-container-platform-logo inline"src="images/LeetCode logo.png" alt="platform-logo">
        </div>
        <div class="checkoverflow inline margin-L-2 width-30">
          <a href="https://codeforces.com/problemset/problem/<%= problemId[i] %>/<%= problemIndex[i] %>" class=" "><%= history[i] %></a>
        </div>


        <% if(language[i]==="GNU C++14" ||language[i]==="GNU C++11" ||language[i]==="GNU C++17"){ %>
          <h6 class="inline margin-L-1 center ">LANGUAGE :</h6>
        <h6 class="inline">C++</h6>
      <%  } %>
      </div>
  <%}  else { %>
      <div class="history-container-div correct-color">
        <div class="history-container-platform-logo-div inline">
          <img class="history-container-platform-logo inline"src="images/LeetCode logo.png" alt="platform-logo">
        </div>
        <div class="checkoverflow inline margin-L-2 width-30">
          <a href="https://codeforces.com/problemset/problem/<%= problemId[i] %>/<%= problemIndex[i] %>" class=" "><%= history[i] %></a>
        </div>
        <h6 class="inline margin-L-1 center ">LANGUAGE :</h6>
        <% if(language[i]==="GNU C++14"){ %>
          <h6 class="inline">C++</h6>
      <%  } %>
      <% if(language[i]==="GNU C11" ){ %>
      <h6 class="inline">C</h6>
    <%  } %>
      </div>
      <%    }   %>
      <%  } %>
    <div class="pagination ">
    <a class="prev" href="" >&laquo;</a>
    <a id="1" href="/history?page=1" class="pagenumber active">1</a>
    <a id="2" href="/history?page=2" class="pagenumber">2</a>
    <a id="3" href="/history?page=3" class="pagenumber">3</a>
    <a id="4" href="/history?page=4" class="pagenumber">4</a>
    <a id="5" href="/history?page=5" class="pagenumber">5</a>
    <a id="6" href="/history?page=6" class="pagenumber">6</a>
    <a class="next" href="">&raquo;</a>
  </div>
    </div>

</section>
      </div>

      <!-- Right Nav -->

      <nav id="r-nav"class="main-menu-r-history">
              <ul>
                  <li class="navbar-brand">

                        <img src="Logo/Notification.png" class="notification-logo">

                    <div class="element toHide">
                      <h2 class="account-name "> <%= username %> </h2>
                    </div>

                      <div class="circle-empty">
                        <img class="avatarImg" src="<%= profilepic %>" alt="">
                      </div>


                  </li>
                  <li id="hideitem1" class="has-subnav nav-item">
                      <a  class="toHighlight" href="#">
                        <img src="Logo/SH-1.png" class="navbar-logo">
                      </a>
                  </li>
                  <li id="hideitem2" class="has-subnav nav-item">
                      <a class="toHighlight" href="#">
                        <img class="navbar-logo" src="images/50.png" alt="" />
                      </a>
                  </li>
                  <div id="hideitem3">
                    <div class="superhero-div">
                        <a id="superheroCornerLink" href="#">SUPERHERO CORNER</a>
                    </div>

                    <li class="nav-item hero-div">
                      <button href="#" type="button" class=" btn colorTochange heroBox">
                        <div class="superheroimg-div">
                          <img src="Logo/SH-1.png" class="superheroImage">
                        </div>
                        <div class="algoCoder-text">
                          <span class="heading-first">ALGO</span><span class="heading-second">CODER</span>
                        </div>
                        <div class="flex justify-content-around trophies toHide">
                          <img class="trophie-image" src="Logo/45.png">
                          <img class="trophie-image" src="Logo/45.png">
                          <img class="trophie-image" src="Logo/45.png">
                        </div>

                      </button>
                    </li>
                    <li class="history-header toHide">

                      <h1>HISTORY OF QUESTIONS</h1>
                    </li>
                    <li class="question-list toHide">
                    <% for(let i=0;i<8;i++) { %>
                    <%  if ( statusinfo[i] === "WRONG_ANSWER" ||statusinfo[i]=== "COMPILATION_ERROR" ){ %>
                        <a type="button" href="https://codeforces.com/problemset/problem/<%= problemId[i] %>/<%= problemIndex[i] %>" class="btn question-box" style="background-color:#f55c47 ;">
                  <% }  else { %>
                        <a type="button" href="https://codeforces.com/problemset/problem/<%= problemId[i] %>/<%= problemIndex[i] %>" class="btn question-box">
                  <%    }   %>

                          <div class="question">
                            <div class="circle">
                              <img src="Logo/CodeForces.png" class="small-circle-img">
                            </div>
                            <div class="question-text">
                              <p> <%= history[i] %> </p>
                            </div>

                          </div>
                        </a>
                    <%  } %>



                  </li>
                  </div>

              </ul>


          </nav>



    <!-- Bootstrap Script -->
 <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.6.0/dist/umd/popper.min.js" integrity="sha384-KsvD1yqQ1/1+IA7gi3P0tyJcT3vR+NdBTt13hSJ2lnve8agRGXTTyNaBYmCR/Nwi" crossorigin="anonymous"></script>
 <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.min.js" integrity="sha384-nsg8ua9HAw1y0W1btsyWgBklPnCUAFLuTMS2G72MMONqmOymq585AcH49TLBQObG" crossorigin="anonymous"></script>
 <script>
 function goBack()
 {
   window.history.back();
 }
$(document).ready(function(){
  $(".pagenumber").click(function(){
    $(".pagination").find(".pagenumber.active").
      addClass('active');
      $(".pagination").find(".pagenumber.active").
        removeClass('active');
  })
  $(".next").click(function(){
    $(".pagination").find(".pagenumber.active").next().
      addClass('active');
      $(".pagination").find(".pagenumber.active").prev().
        removeClass('active');
  })
  $(".prev").click(function(){
    $(".pagination").find(".pagenumber.active").prev().
      addClass('active');
      $(".pagination").find(".pagenumber.active").next().
        removeClass('active');
  })
})
 </script>

</body>

</html>
