<%- include('partials/header') %>
    <!-- LEFT NAVBAR -->
    <nav id="l-nav"class="main-menu">
            <ul>
                <li>
                  <div class=" has-subnav navbar-brand">
                    <h2><span class="heading-first">C</span><span id="hidespan1" class="heading-first hide">ODE</span></span><span class="heading-second">B</span><span id="hidespan2"class="heading-second hide">REAKER</span></h2>
                  </div>

                </li>
                <li class="has-subnav nav-item">
                    <a  class="toHighlight" href="/">
                      <img class="icon-image" src="Logo/Dashboard.png" alt="">
                      <span class="nav_name">DASHBOARD</span>
                    </a>

                </li>
                <li class="has-subnav nav-item">
                    <a class="toHighlight" href="/CodeItOut">
                      <img class="icon-image" src="Logo/CodeItOut.png" alt="">
                      <span class="nav_name">CODE IT OUT</span>
                    </a>

                </li>
                <li class="has-subnav nav-item">
                    <a class="toHighlight" href="/brocode">
                      <img class="icon-image" src="Logo/BroCode.png" alt="">
                      <span class="nav_name">BRO CODE</span>
                    </a>

                </li>
                <li class="nav-item">
                    <a class="toHighlight" href="/">

                        <img class="icon-image" src="Logo/LearnBros.png" alt="">
                        <span class="nav_name">LEARN BROS</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="toHighlight" href="/">

                        <img class="icon-image" src="Logo/Premium.png" alt="">
                        <span class="nav_name">PREMIUM</span>
                    </a>
                </li>

            </ul>

            <ul class="logout">
                <li>
                     <a class="navbar-titles toHighlight" href="#">
                       <img class="icon-image" src="Logo/Logo.png" alt="logo">
                       <span class="nav_name">SETTINGS</span>
                     </a>
                </li>
            </ul>
        </nav>

        <!-- PORTFOLIO DASHBOARD -->

      <div class="content">
        <section id="portfolio">
          <div class="top-div">
            <div class="">
              <h3 class="heading">PORTFOLIO</h3>
            </div>
            <div class="input-group">
              <input type="search" class="form form-control shadow-sm" placeholder="SEARCH ANYTHING YOU WANT" aria-label="Search" aria-describedby="search-addon" />
              <span class="input-group-text border-0" id="search-addon">
              <i class="fas fa-search"></i>
              </span>
            </div>

          </div>


        <!-- Stats -->

        <div class="stats">
          <div class="container-1 d-flex row-cols-3">
            <button type="button" class="btn flexbox justify-content-start">
              <h5 class="flex-heading">TOTAL Q'S ATTEMPTED</h5>
            <div class="sigma">
              <img class="flex-image img-fluid rounded float-start" src="images\sigma.png" alt="sigma-image">
            </div>
              <p class="Number-of-questions"> <%= totalSubmission %> </p>
            </button>


            <button type="button" class="btn flexbox justify-content-center">
              <h5 class="flex-heading">CORRECTLY ANSWERED</h5>
            <div class="sigma">
              <img class="flex-image img-fluid rounded float-start" src="images\check.png" alt="sigma-image">
            </div>
              <p class="Number-of-questions"> <%= correctsubmission %></p>
            </button>

            <button type="button" class="btn flexbox justify-content-end">
              <h5 class="flex-heading">EFFICIENCY</h5>
            <div class="sigma">
              <img class="flex-image img-fluid rounded float-start" src="images\efficiency.png" alt="sigma-image">
            </div>
              <p class="Number-of-questions"> <%= efficiency %> %</p>
              <div class="efficiency-meter ">
                <div id="myBar" class="efficiencyPercentage" ></div>
              </div>
            </button>
          </div>
          <div class="container-2 d-flex row-cols-2">
            <div class="graphflex">
              <div class="graphflex-heading">
                <h6 class="question-division"><span><img class="stack-logo" src="images/graph (2).png" alt="graph-img"></span>QUESTIONS DIVISION</h6>
              </div>
              <div class="container">
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">NO.THEORY</h6>
                      <p> <%= noTheory %> Q's </p>
                    </button>
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">MATH</h6>
                      <p><%= math %> Q's </p>
                    </button>
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">GREEDY</h6>
                      <p> <%= greedy %> Q's </p>
                    </button>
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">GRAPHS</h6>
                      <p> <%= graph %> Q's </p>
                    </button>
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">DP</h6>
                      <p> <%= dp %> Q's </p>
                    </button>
                    <button type="button" class="btn divisionbox">
                      <h6 class="division-name">OTHER</h6>
                      <p> <%= other %> Q's</p>
                    </button>
                </div>
              <!-- <img class="graph-image"src="images/graph (1).png" alt="graph-img"> -->
              <div class="graph-image">
                <canvas id="myChart" width="100" height="100" style="display:inline-block;"></canvas>
              </div>

              </div>
              <div class="platform-div">
                <div class="piechart-heading">
                  <h6 class="platform-division"><span><img class="stack-logo" src="images/graph (2).png" alt="graph-img"></span>PLATFORM DIVISION</h6>
                </div>

                <div class="piechart-image">
                  <canvas id="pieChart" width="100" height="100" style="display:inline-block;"></canvas>
                </div>


                </div>
              </div>
        </div>

        </section>
      </div>

      <nav id="r-nav"class="main-menu-r">
              <ul>
                  <li class="navbar-brand">

                        <img src="Logo/Notification.png" class="notification-logo">

                    <div class="element toHide">
                      <h2 class="account-name ">MR. <%= username %> </h2>
                    </div>

                      <div class="circle-empty">
                        <img class="avatarImg" src="<%= profilepic %>" alt="">
                      </div>


                  </li>
                  <li id="hideitem1" class="has-subnav nav-item">
                      <a  class="toHighlight" href="#">
                        <img src="Logo/SH-1.png" class="navbar-logo">
                      </a>
                  </li>
                  <li id="hideitem2" class="has-subnav nav-item">
                      <a class="toHighlight" href="#">
                        <img class="navbar-logo" src="images/50.png" alt="" />
                      </a>
                  </li>
                  <div id="hideitem3">
                    <div class="superhero-div">
                        <a id="superheroCornerLink" href="#">SUPERHERO CORNER</a>
                    </div>

                    <li class="nav-item hero-div">
                      <button href="#" type="button" class=" btn colorTochange heroBox">
                        <div class="superheroimg-div">
                          <img src="Logo/SH-1.png" class="superheroImage">
                        </div>
                        <div class="algoCoder-text">
                          <span class="heading-first">ALGO</span><span class="heading-second">CODER</span>
                        </div>
                        <div class="flex justify-content-around trophies toHide">
                          <img class="trophie-image" src="Logo/45.png">
                          <img class="trophie-image" src="Logo/45.png">
                          <img class="trophie-image" src="Logo/45.png">
                        </div>

                      </button>
                    </li>
                    <li class="history-header toHide">

                      <h1>HISTORY OF QUESTIONS</h1>
                    </li>
                    <li class="question-list toHide">
                    <% for(let i=0;i<8;i++) { %>
                    <%  if ( statusinfo[i] === "WRONG_ANSWER" ||statusinfo[i]=== "COMPILATION_ERROR" ){ %>
                        <button type="button" class="btn question-box" style="background-color:#f55c47 ;">
                  <% }  else { %>
                        <button type="button" class="btn question-box">
                  <%    }   %>

                          <div class="question">
                            <div class="circle">
                              <img src="Logo/CodeForces.png" class="small-circle-img">
                            </div>
                            <div class="question-text">
                              <p> <%= history[i] %> - C++</p>
                            </div>

                          </div>
                        </button>
                    <%  } %>



                  </li>
                  </div>

              </ul>


          </nav>

      <script>

      const ctx = document.getElementById('myChart').getContext('2d');
      const myChart = new Chart(ctx, {
          type: 'bar',
          data: {
              labels: ['NO. THEORY', 'MATH', 'GREEDY', 'GRAPHS', 'DP', 'OTHER'],
              datasets: [{
                  label: 'No. of Question',
                  data: [<%= noTheory %>,<%=math%>, <%=greedy%>, <%=graph%>, <%=dp%>,<%=other%> ],
                  backgroundColor: [
                      'rgba(255, 99, 132, 0.7)',
                      'rgba(54, 162, 235, 0.7)',
                      'rgba(255, 206, 86, 0.7)',
                      'rgba(75, 192, 192, 0.7)',
                      'rgba(153, 102, 255, 0.7)',
                      'rgba(255, 159, 64, 0.7)'
                  ],
                  borderColor: [
                      'rgba(255, 99, 132, 1)',
                      'rgba(54, 162, 235, 1)',
                      'rgba(255, 206, 86, 1)',
                      'rgba(75, 192, 192, 1)',
                      'rgba(153, 102, 255, 1)',
                      'rgba(255, 159, 64, 1)'
                  ],
                  borderWidth: 1
              }]
          },
          options: {

            scales: {
                  y: {
                      beginAtZero: true
                  }
              },
              plugins:{
                legend:{
                display:false
                }
              }


          }
      });
      // PIECHART
      var pie = document.getElementById("pieChart").getContext('2d');
      var piechart = new Chart(pie, {
      type: 'pie',
      data: {
      labels: ['NO. THEORY', 'MATH', 'GREEDY', 'GRAPHS', 'DP', 'OTHER'],
      datasets: [{
      backgroundColor: [
        "#2ecc71",
        "#3498db",
        "#95a5a6",
        "#9b59b6",
        "#f1c40f",
        "#e74c3c",
        "#34495e"
      ],
      data: [<%= noTheory %>, <%=math%>, <%=greedy%>, <%=graph%>, <%=dp%>,<%=other%> ],

      hoverOffset:4
    }]
  }
});

var i = 0;
function move() {
  if (i == 0) {
    i = 1;
    var elem = document.getElementById("myBar");
    var width = 1;
    var id = setInterval(frame, 10);
    function frame() {
      if (width >= <%= efficiency %>) {
        clearInterval(id);
        i = 0;
      } else {
        width++;
        elem.style.width = width + "%";
      }
    }
  }
}
move();
      </script>



    <!-- Bootstrap Script -->
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.6.0/dist/umd/popper.min.js" integrity="sha384-KsvD1yqQ1/1+IA7gi3P0tyJcT3vR+NdBTt13hSJ2lnve8agRGXTTyNaBYmCR/Nwi" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.min.js" integrity="sha384-nsg8ua9HAw1y0W1btsyWgBklPnCUAFLuTMS2G72MMONqmOymq585AcH49TLBQObG" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <script src="index.js" charset="utf-8"></script>

  </body>
</html>
